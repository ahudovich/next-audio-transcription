{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^next",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@/env",
    "^@/",
    "^[.]",
    "<TYPES>^next$",
    "<TYPES>^react$",
    "<TYPES>",
    "<TYPES>^@/",
    "<TYPES>^[.]"
  ]
}
